<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.admin.conurbations">

    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_FORMAT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.SET_WALLPAPER" />

    <application
        android:name=".widget.SolidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activitys.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.ItemListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="com.tangxiaolv.telegramgallery.GalleryActivity" />
        <activity android:name=".activitys.ContactsActivity" />
        <activity android:name=".activitys.TelegramGalleryActivity" />
        <activity
            android:name=".activitys.BigAvatarActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustNothing"/>
        <activity
            android:name=".activitys.ItemDetailActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.ShowImageActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.ItemListItemActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.DayAndDayPrettyPicturesMoreActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.PersonalInformationActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.WebviewActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.NewsDetailActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.AboutActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.SearchActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>